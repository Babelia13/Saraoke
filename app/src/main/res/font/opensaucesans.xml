<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:targetApi="o">
    <font
        android:font="@font/opensaucesans_light"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/opensaucesans_light"
        app:fontStyle="normal"
        app:fontWeight="300" />
    <font
        android:font="@font/opensaucesans_lightitalic"
        android:fontStyle="italic"
        android:fontWeight="300"
        app:font="@font/opensaucesans_lightitalic"
        app:fontStyle="italic"
        app:fontWeight="300" />
    <font
        android:font="@font/opensaucesans_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/opensaucesans_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />
    <font
        android:font="@font/opensaucesans_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/opensaucesans_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />
    <font
        android:font="@font/opensaucesans_semibold"
        android:fontStyle="normal"
        android:fontWeight="600"
        app:font="@font/opensaucesans_semibold"
        app:fontStyle="normal"
        app:fontWeight="600" />
    <font
        android:font="@font/opensaucesans_semibolditalic"
        android:fontStyle="italic"
        android:fontWeight="600"
        app:font="@font/opensaucesans_semibolditalic"
        app:fontStyle="italic"
        app:fontWeight="600" />
    <font
        android:font="@font/opensaucesans_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/opensaucesans_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />
    <font
        android:font="@font/opensaucesans_bolditalic"
        android:fontStyle="italic"
        android:fontWeight="700"
        app:font="@font/opensaucesans_bolditalic"
        app:fontStyle="italic"
        app:fontWeight="700" />
    <font
        android:font="@font/opensaucesans_extrabold"
        android:fontStyle="normal"
        android:fontWeight="800"
        app:font="@font/opensaucesans_extrabold"
        app:fontStyle="normal"
        app:fontWeight="800" />
    <font
        android:font="@font/opensaucesans_extrabold"
        android:fontStyle="italic"
        android:fontWeight="800"
        app:font="@font/opensaucesans_extrabold"
        app:fontStyle="italic"
        app:fontWeight="800" />
</font-family>